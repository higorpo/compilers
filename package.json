  "scripts": {
    "watch": "nodemon --ignore node_modules --watch './src/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/main.ts",
    "dev": "tsc-watch --onSuccess \"npm run watch\""
  },
  "dependencies": {
    "antlr4": "^4.11.0"
  },
  "devDependencies": {
    "@types/antlr4": "^4.11.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3"
  }
}
